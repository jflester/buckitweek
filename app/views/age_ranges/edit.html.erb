<h1>Editing age_range</h1>

<% form_for(@age_range) do |f| %>
  <p>
    <%= f.label :low %><br />
    <%= f.text_field :low %>
  </p>
  <p>
    <%= f.label :high %><br />
    <%= f.text_field :high %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @age_range %> |
<%= link_to 'Back', age_ranges_path %>